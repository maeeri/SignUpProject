        <h1>@Localizer["title"]</h1>
    </div>
</div>

<div class="text-center">
    @if (User.Identity.IsAuthenticated)
    {
        <h3>@Localizer["welcome"] @User.Identity.Name</h3>
        
    }
    else
    {
        <h1 class="display-4">@Localizer["new welcome"]</h1>
    }
    
